        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:views="clr-namespace:Homework_8.Views"
        xmlns:pages="clr-namespace:Homework_8.Views.Pages"
        xmlns:local="clr-namespace:v.ViewModels"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="Homework_8.Views.MainWindow"
        Title="Homework_8">
  
  <Window.DataTemplates>
    <DataTemplate DataType="vm:DataGridViewModel">
